{
  "name": "archlens-in-vscode",
  "displayName": "ArchLens In VSCode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "files": [
    "out/**",
    "webview/**"
  ],
  "extensionDependencies": [
    "ms-python.python"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "archlens-in-vscode.openGraphView",
        "title": "Open Graph-view"
      },
      {
        "command": "archlens-in-vscode.openFile",
        "title": "Open File"
      },
      {
        "command": "archlens-in-vscode.setupArchLens",
        "title": "Set up ArchLens"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "filesWithDependencyTree",
          "name": "Dependency Tree"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  }
}
